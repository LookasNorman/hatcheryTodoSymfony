{% extends 'base.html.twig' %}

{% block title %}ObjectAddress{% endblock %}

{% block body %}
    <h1>ObjectAddress</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ object_address.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ object_address.name }}</td>
            </tr>
            <tr>
                <th>Street</th>
                <td>{{ object_address.street }}</td>
            </tr>
            <tr>
                <th>StreetNumber</th>
                <td>{{ object_address.streetNumber }}</td>
            </tr>
            <tr>
                <th>PhoneNumber</th>
                <td>{{ object_address.phoneNumber }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ object_address.email }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('object_address_index') }}">back to list</a>

    <a href="{{ path('object_address_edit', {'id': object_address.id}) }}">edit</a>

    {{ include('object_address/_delete_form.html.twig') }}
